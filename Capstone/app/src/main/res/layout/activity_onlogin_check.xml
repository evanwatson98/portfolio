<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".onLoginCheck">


    <TextView
        android:id="@+id/loginhouseID"
        android:layout_width="207dp"
        android:layout_height="29dp"
        android:layout_marginStart="97dp"
        android:layout_marginTop="153dp"
        android:layout_marginEnd="169dp"
        android:hint="houseID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/loginuserID"
        android:layout_width="210dp"
        android:layout_height="21dp"
        android:layout_marginStart="97dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="163dp"
        android:hint="userID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loginhouseID" />

    <TextView
        android:id="@+id/messageBack"
        android:layout_width="201dp"
        android:layout_height="42dp"
        android:layout_marginStart="62dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="148dp"
        android:hint="messasge back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="460dp"
        android:layout_height="760dp"
        android:background="#AAD18D"
        tools:layout_editor_absoluteX="36dp" />

    <RelativeLayout
        android:id="@+id/loadingPanel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        tools:layout_editor_absoluteX="-29dp"
        tools:layout_editor_absoluteY="0dp">

        <ProgressBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:theme="@style/AppTheme.WhiteAccent" />




    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>